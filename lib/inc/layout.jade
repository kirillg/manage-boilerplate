doctype html
html(lang='en', hola_ext_inject='disabled')
  head
    title Auth0
    meta(charset='UTF-8')
    meta(http-equiv='X-UA-Compatible', content='IE=Edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    link(rel='shortcut icon', href='https://cdn.auth0.com/styleguide/2.0.1/lib/logos/img/favicon.png')
    meta(name='description', content='auth0/manage boilerplate')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    link(rel='apple-touch-icon', href='apple-touch-icon.png')
    link(rel='stylesheet', type='text/css', href='https://cdn.auth0.com/manage/v0.3.973/css/index.min.css')
    link(rel='stylesheet', type='text/css', href='main.css')
    script(type='text/javascript', src='https://cdn.auth0.com/manage/v0.3.973/components/ZeroClipboard/ZeroClipboard.js')
    script(type='text/javascript', src='https://cdn.auth0.com/manage/v0.3.973/js/bundle.js')
  body.a0-extension
    header.dashboard-header
      nav.navbar.navbar-default(role='navigation')
        .container
          .navbar-header
            h1.navbar-brand
              a(href='http://manage.auth0.com/')
                span Auth0
          #navbar-collapse.collapse.navbar-collapse
            ul.nav.navbar-nav.navbar-right
              li
                a(target='_blank', href='https://auth0.com/support') Help & Support
              li
                a(target='_blank', href='https://auth0.com/docs') Documentation
              li.dropdown
                span.btn-dro.btn-username(role='button', data-toggle='dropdown', data-target='#')
                  img.picture.avatar(src='https://secure.gravatar.com/avatar/a12075a065d2c41ac447dcffc1398d43?s=480&r=pg&d=https%3A%2F%2Fcdn.auth0.com%2Favatars%2Fro.png')
                  span.username-text.truncate Username
                  i.icon-budicon-460
                ul.dropdown-menu.animated(role='menu', aria-labelledby='dLabel')
                  li(role='presentation')
                    a(role='menuitem', tabindex='-1', href='#/account') Account settings
                  li.divider(role='presentation')
                  li(role='presentation')
                    a.new_account(role='menuitem', tabindex='-1', href='#') New Account
                  li.divider(role='presentation')
                  li(role='presentation')
                    a(role='menuitem', tabindex='-1', href='/logout') Logout
    .container
      .row
        #sidebar.col-xs-2
          .sidebar-fixed
            ul
              li
                i.icon.icon-budicon-497
                span Dashboard
              li
                i.icon.icon-budicon-143
                span Apps / APIs
              li
                i.icon.icon-budicon-339
                span Connections
              li
                i.icon.icon-budicon-292
                span Users
              li.active
                a
                  i.icon.icon-budicon-292
                  span Groups
              li
                i.icon.icon-budicon-173
                span Rules
              li
                i.icon.icon-budicon-243
                span Multifactor Auth
              li
                i.icon.icon-budicon-725
                span Login Page
              li
                i.icon.icon-budicon-778
                span Emails
              li
                i.icon.icon-budicon-754
                span Logs
              li
                i.icon.icon-budicon-324
                span Extensions
              li
                i.icon.icon-budicon-324
                span Get Support

        .col-xs-10.wrapper
          block content
